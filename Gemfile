  gem "jekyll-feed", "~> 0.6"

  # Milti Language
  gem 'jekyll-multiple-languages-plugin'

  # SEO
  gem 'jekyll-seo-tag'

  # SCSS
  gem 'jekyll-sass-converter'
end

